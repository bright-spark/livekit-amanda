# Azure OpenAI Configuration
AZURE_OPENAI_API_KEY=your_azure_openai_api_key
AZURE_OPENAI_ENDPOINT=your_azure_openai_endpoint
AZURE_OPENAI_DEPLOYMENT=your_azure_openai_deployment
AZURE_OPENAI_VERSION=your_azure_openai_version

# Azure Speech Services Configuration
AZURE_STT_API_KEY=your_azure_stt_api_key
AZURE_STT_REGION=your_azure_stt_region
AZURE_TTS_API_KEY=your_azure_tts_api_key
AZURE_TTS_REGION=your_azure_tts_region

# Proxy Configuration
PROXY_URL=your_proxy_url

# Brave Search Configuration
# Persistent Cache API key
BRAVE_PERSISTENT_CACHE_API_KEY=your_brave_persistent_cache_api_key

# Web Search API keys
# Multiple web search API keys for parallel processing
BRAVE_WEB_SEARCH_API_KEY=your_brave_web_search_api_key
BRAVE_WEB_SEARCH_API_KEY_1=your_first_brave_web_search_api_key
#BRAVE_WEB_SEARCH_API_KEY_2=your_second_brave_web_search_api_key
#BRAVE_WEB_SEARCH_API_KEY_3=your_third_brave_web_search_api_key

# AI Search API keys
# Multiple AI search API keys for parallel processing
BRAVE_AI_SEARCH_API_KEY=your_brave_ai_search_api_key
BRAVE_AI_SEARCH_API_KEY_1=your_first_brave_ai_search_api_key
#BRAVE_AI_SEARCH_API_KEY_2=your_second_brave_ai_search_api_key

# Grounding API keys - primary key and multiple keys for parallel processing
BRAVE_GROUNDING_API_KEY=your_brave_grounding_api_key
BRAVE_GROUNDING_API_KEY_1=your_first_brave_grounding_api_key
#BRAVE_GROUNDING_API_KEY_2=your_second_brave_grounding_api_key
#BRAVE_GROUNDING_API_KEY_3=your_third_brave_grounding_api_key

# Brave Search Enable Cache
BRAVE_SEARCH_ENABLE_CACHE=true

# Brave Search Enable Persistence
BRAVE_SEARCH_ENABLE_PERSISTENCE=true

# Brave Search Rate Limits
BRAVE_WEB_SEARCH_RATE_LIMIT=1  # 1 for free tier, 20 for paid tier
BRAVE_AI_SEARCH_RATE_LIMIT=1   # 1 for free tier, 20 for paid tier
BRAVE_PERSISTENT_CACHE_RATE_LIMIT=1  # 1 for free tier, 20 for paid tier
BRAVE_GROUNDING_RATE_LIMIT=1  # 1 for free tier, 20 for paid tier

# OpenWeather Configuration
OPENWEATHER_API_KEY=your_openweather_api_key

# Zapier MCP Configuration
ZAPIER_MCP_URL=your_zapier_mcp_url

